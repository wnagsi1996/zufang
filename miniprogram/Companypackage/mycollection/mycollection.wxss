/* Companypackage/mycollection/mycollection.wxss */
.coll-dox{
  margin: 30rpx auto;
  background-color: #fff;
  display: flex;
  padding:30rpx
}
.coll-image{
  width: 200rpx;
  height: 200rpx;
}
.coll-image image{
  width: 100%;
  height: 100%;
}
.coll-info{
  flex: 1;
  padding-left: 20rpx;
}
.coll-info view{
  padding-bottom:10rpx;
  font-size: 24rpx;
}
.coll-info .name{
  font-weight: bold;
  font-size: 28rpx;
}
.del{
  width: 130rpx;
  height: 260rpx;
  line-height: 260rpx;
  text-align: center;
  background-color: red;
  color: #fff;
}