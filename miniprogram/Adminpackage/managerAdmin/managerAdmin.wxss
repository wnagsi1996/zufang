/* Adminpackage/managerAdmin/managerAdmin.wxss */
.container{
  margin:25rpx;
}
.header{
  border-radius: 10rpx;
  padding:20rpx;
  background-color: #fff;
}
.header .title{
  font-weight: bold;
}
.header .desc{
  padding:5rpx 0
}
.add-btn{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin:25rpx 0;
  text-align: center;
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: center;
}
.add-btn image{
  width: 60rpx;
  height: 60rpx;
}
.user-item{
  height: 140rpx;
  padding:20rpx;
  background-color: #fff;
  margin:25rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.user-item .left{
  flex: 1;
}
.user-item .right{
  width: 60rpx;

}
.user-item .left .name{
  font-weight: bold;
}
.user-item .left view{
  padding: 6rpx 0;
}
.user-item .left .txt{
  color: #888;
}

.user-del{
  width: 130rpx;
  height: 180rpx;
  line-height: 180rpx;
  background-color: red;
  color: #fff;
  text-align: center;
}
.qr-box{
  margin: 25rpx;
  padding: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
  text-align: center;
}
.qr-box .title{
  text-align: left;
}
.qr-box image{
  width: 500rpx;
  height: 500rpx;
  margin: 20rpx auto;
}