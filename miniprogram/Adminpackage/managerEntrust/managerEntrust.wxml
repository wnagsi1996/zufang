<!--Adminpackage/managerEntrust/managerEntrust.wxml-->
<van-tabs active="{{ active }}" bind:change="onChange">
  <van-tab title="待审核" />
  <van-tab title="已发布" />
</van-tabs>

<view class="list-box">
  <view class="list-item" wx:for="{{CheckingEntrustList}}" wx:key="_id">
    <view class="name"><text>{{item.FormData.name}}</text></view>
    <view>委托人联系方式：<text>{{item.FormData.phonenumber}}</text></view>
    <view>委托发布时间：<text>{{item.updateTime}}</text></view>
    <view>委托类型：<text>{{item.EntrustType=="sale"?'出售':'出租'}}</text></view>
    <view>审核状态:<text>待审核</text></view>
    <view>发布状态:<text>待发布</text></view>
    <view>房源详细地址:<text>{{item.FormData.detailLocation}}</text></view>
  </view>
</view>