/* miniprogram/pages/login/login.wxss */
.coninter{
  width: 700rpx;
  margin: 50rpx auto;
  border-radius: 10rpx;
  background-color: #fff;
  padding:20rpx 0;
}
.btn{
  width: 650rpx;
  margin:30rpx auto;
  text-align: center;
}
.wechat{
  text-align: center;
  margin-top: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.wechat text{
  padding-top: 10rpx;
}
.wechat .icon{
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 50rpx;
  background-color: #04BE02;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.login-bj{
  width: 750rpx;
  height: 100vh;
  background-color: rgba(0,0,0,0.5);
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  
}
.login-dox{
  position: relative;
}
.login-bj image{
  width: 600rpx;
  height:650rpx;
}
.login-bj .btn{
  width: 550rpx;
  position: absolute;
  bottom: 0;
  left: 25rpx;
}