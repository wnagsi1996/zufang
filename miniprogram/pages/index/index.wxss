.select-box{
  width: 750rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
}
.select-box .item{
  flex: 1;
  text-align: center;
}
.select-box .item image{
  width: 200rpx;
  height: 200rpx;
}
.menu-box{
  width: 750rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding: 30rpx 0;
  background-color: #fff;
}
.menu-box .item{
  width:25%;
  height: 160rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.menu-box .item image{
  width: 80rpx;
  height: 80rpx;
}
.menu-box .item text{
  margin-top: 10rpx;
}
.banner{
  padding:0 30rpx;
  background-color: #fff;
}
.banner image{
  width: 100%;
  height: 140rpx;
}
.list-name{
  width: 720rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  padding-left: 30rpx;
  margin-top: 20rpx;
  font-weight: bold;
}